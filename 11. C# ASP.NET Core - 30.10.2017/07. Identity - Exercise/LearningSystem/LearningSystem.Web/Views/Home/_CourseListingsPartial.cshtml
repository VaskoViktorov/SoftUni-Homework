@model IEnumerable<CourseListingServiceModel>

@foreach (var course in Model)
{
    <div class="col-md-4">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title tex"><a asp-controller="Courses" asp-action="Details" asp-route-id="@course.Id">@course.Name</a></h3>
            </div>
            <div class="panel-body">
                @course.StartDate.ToShortDateString() - @course.EndDate.ToShortDateString()
            </div>
        </div>
    </div>
}
